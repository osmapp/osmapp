{
  "name": "osmapp",
  "scripts": {
    "dev": "next",
    "lint": "eslint --print-config . | eslint-config-prettier-check && eslint . --report-unused-disable-directives",
    "build": "next build",
    "start": "next start",
    "dist": "next build && next export -o dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
