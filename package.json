    "@sentry/browser": "^5.11.2",
    "@sentry/node": "^5.11.2",
    "autosuggest-highlight": "^3.1.1",
    "isomorphic-unfetch": "^3.0.0",
    "isomorphic-xml2js": "^0.1.3",
    "jest": "^25.1.0",
    "js-cookie": "^2.2.1",
    "jss": "^10.0.4",
    "leaflet": "^1.6.0",
    "lodash": "^4.17.19",
    "mapbox-gl": "^1.7.0",
    "next": "^9.2.1",
    "next-cookies": "^2.0.3",
    "react": "^16.12.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.12.0",
