    "styled-jsx": "^3.2.4"
  },
  "devDependencies": {
    "@kiwicom/eslint-config": "^8.4.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-styled-components": "^1.10.7",
    "eslint": "^6.8.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7"
  }
